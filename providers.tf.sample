provider "google" {
  version = "~> 2.7"
  region  = "${var.region}"
  project = "${var.project}"
  zone    = "${var.zone}"
}

provider "google-beta" {
  version = "~> 2.7"
  region  = "${var.region}"
  zone    = "${var.zone}"
  project = "${var.project}"
}

provider "acme" {
  version = "~> 1.3"
  server_url = "${var.acme_server}"
}

provider "random" {
  version = "~> 2.1"
}

provider "local" {
  version = "~> 1.2"
}

provider "tls" {
  version = "~> 2.0"
}
